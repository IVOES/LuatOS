{
  "workdir": "D:/github/LuatOS/",
  "as": ["md"],
  "entries": [{
    "title": "LuaAPI模块",
    "path": "luat/modules",
    "suffix": [".c"],
    "lang" : ["LUA"],
    "dropEmptyComment": true,
    "out": "lua",
    "deep": true
  }, {
    "title": "FreeRTOS集成",
    "path": "luat/freertos",
    "suffix": [".c"],
    "lang" : ["C"],
    "dropEmptyComment": true,
    "out": "c_api/${name}",
    "deep": true
  }, {
    "title": "RT-Thread集成",
    "path": "luat/rtt",
    "suffix": [".c"],
    "lang" : ["C"],
    "dropEmptyComment": true,
    "out": "c_api/${name}",
    "deep": true
  }],
  "output": "${workdir}/docs/api/"
}