{
  "workdir": "D:/github/",
  "as": ["md"],
  "entries": [{
    "title": "LuaAPI模块",
    "paths": [
        "LuatOS/luat/modules",
        "LuatOS/luat/rtt",
        "air302/sdk/luat" 
    ],
    "suffix": [".c"],
    "lang" : ["LUA"],
    "dropEmptyComment": true,
    "out": "lua",
    "deep": true
  }, {
    "title": "FreeRTOS集成",
    "paths": ["LuatOS/luat/freertos"],
    "suffix": [".c"],
    "lang" : ["C"],
    "dropEmptyComment": true,
    "out": "c_api/${name}",
    "deep": true
  }, {
    "title": "RT-Thread集成",
    "paths": ["LuatOS/luat/rtt"],
    "suffix": [".c"],
    "lang" : ["C"],
    "dropEmptyComment": true,
    "out": "c_api/${name}",
    "deep": true
  }],
  "output": "${workdir}/LuatOS/docs/api/"
}