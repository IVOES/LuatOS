{
  "rawText": "=INPUT",
  "summary": "ntp时间同步",
  "modifier": null,
  "returns": [{
      "type": "int",
      "comment": "启动成功返回0, 失败返回1或者2"
    }],
  "name": "socket.ntpSync",
  "params": [{
      "type": "string",
      "name": "server",
      "comment": "ntp服务器域名,默认值ntp1.aliyun.com"
    }],
  "examples": [{
    "summary": ["如果读取失败,会返回nil"],
    "code": [
      "socket.ntpSync()",
      "sys.subscribe(\"NTP_UPDATE\", function(re)",
      "    log.info(\"ntp\", \"result\", re)",
      "end)"]
  }]
}
